{{ attach_library('shop/contact-styling') }}
<!-- contact section start -->
<form method="post">
  <div class="contact_section layout_padding">
    <div class="container">
      <h1 class="contact_taital">Contact Us</h1>
      <div class="contact_section_2">
        <div class="mail_section_1">
          {# {{ data.form }}# - Không có <form>} #}

          {{ data.form.contact_name }}
          {{ data.form.contact_email }}
          {{ data.form.contact_phone }}
          {{ data.form.contact_message }}
          <div class="send_bt">
            {{ data.form.contact_submit }}
          </div>
        </div>
      </div>
    </div>
  </div>
  {{ data.form.form_id }}
  {{ data.form.form_build_id }}
  {{ data.form.form_token }}
</form>
<!-- contact section end -->




