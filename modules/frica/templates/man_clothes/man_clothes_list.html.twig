
{{ attach_library('shop/computers-to-end-styling') }}
<!-- man_clothes section start -->
<div class="computers_section_2">
  <div class="container-fluid">
    <div class="computer_main">
      <div class="row">
        {% for man in data.man_clothes %} {#data.productsForCurrentPage#}
          <div class="col-md-4">
            <div class="computer_img"><a href="{{ url('frica.product_detail', {'idSP': man.idSP}) }}"><img src="{{ file_url(directory ~ '/images/' ~ man.img) }}"></a></div>
            <h4 class="computer_text">{{ man.idLSP }}</h4>
            <div class="computer_text_main">
              <h4 class="dell_text"><a href="{{ url('frica.product_detail', {'idSP': man.idSP}) }}">{{ man.name }}</a></h4>
              <h6 class="price_text">${{ man.promotional_price|number_format(0, '.', ',') }}</h6>
              <h6 class="price_text_1">${{ man.price|number_format(0, '.', ',') }}</h6>
            </div>
            <div class="cart_bt_1"><a href="{{ url('frica.cart', {'idSP': man.idSP}) }}">Add To Cart</a></div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
<!-- man_clothes section end -->

