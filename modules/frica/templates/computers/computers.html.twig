
{{ attach_library('shop/computers-styling') }}
<!-- computers section start -->
<div class="computers_section layout_padding">
  <div class="container">
    <h1 class="computers_taital">Computer</h1>
  </div>
</div>
<div class="computers_section_2">
  <div class="container-fluid">
    <div class="computer_main">
      <div class="row">
        {% for computer in data.computers %} {#data.productsForCurrentPage#}
          <div class="col-md-4">
            <div class="computer_img"><a href="{{ url('frica.product_detail', {'idSP': computer.idSP}) }}"><img src="{{ file_url(directory ~ '/images/' ~ computer.img) }}"></a></div>
            <h4 class="computer_text">{{ computer.idLSP }}</h4>
            <div class="computer_text_main">
              <h4 class="dell_text"><a href="{{ url('frica.product_detail', {'idSP': computer.idSP}) }}">{{ computer.name }}</a></h4>
              <h6 class="price_text">${{ computer.promotional_price|number_format(0, '.', ',') }}</h6>
              <h6 class="price_text_1">${{ computer.price|number_format(0, '.', ',') }}</h6>
            </div>
            <div class="cart_bt_1"><a href="{{ url('frica.cart', {'idSP': computer.idSP}) }}">Add To Cart</a></div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
<!-- computers section end -->

